# Environment variables example for IMS backend
# Copy this file to .env and edit values for your environment

# Allowed origins (comma-separated). Example: https://your-frontend.com,http://localhost:5173
ALLOWED_ORIGINS="https://ihsanmhd-mr.github.io,http://localhost:5173"

# Development front-end origins used by corsOptions (comma-separated)
DEV_FRONTEND_ORIGINS="http://localhost:5173,http://localhost:4173"

# Node environment
NODE_ENV=development

# Server port
PORT=3000

# Database URL / connection variables (example for Postgres)
# DATABASE_URL=postgres://user:password@host:port/dbname
